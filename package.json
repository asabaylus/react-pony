{
  "name": "react-pony",
  "version": "0.0.1",
  "description": "Sample Component for React Demos",
  "author": "Asa Baylus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:asabaylus/react-pony.git"
  },
  "main": "dist/index.js",
  "module": "lib/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "test": "react-scripts test --env=jsdom",
    "build": "rollup -c --environment NODE_ENV:production",
    "start": "rollup -c -w",
    "prepublish": "yarn build",
    "predeploy": "cd example && yarn install && yarn build",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {
    "whatwg-fetch": "^2.0.3"
  },
  "peerDependencies": {
    "react": "^0.14.9 || ^15.0.0 || ^16.0.0",
    "react-dom": "^0.14.9 || ^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "gh-pages": "^1.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "^1.0.17",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-conditional": "^1.1.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.5.5",
    "rollup-plugin-strip-prop-types": "^1.0.2"
  },
  "files": [
    "dist",
    "lib"
  ]
}
